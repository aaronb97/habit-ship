{
  "expo": {
    "name": "HabitShip",
    "slug": "habit-ship",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "habit-ship",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.habit-ship",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "habit.ship"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0b0f26",
          "image": "./assets/splash-icon.png"
        }
      ],
      "react-native-edge-to-edge",
      "react-native-bottom-tabs"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "c6bdffae-04e5-4726-912e-f3552dbaaf69"
      }
    },
    "owner": "aaron9becker7"
  }
}
